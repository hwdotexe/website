{
  "name": "hadenwatne",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build-css && ng serve",
    "build-css": "tailwindcss -i ./src/tailwind.css -o ./src/generated-tailwind.css --minify",
    "build": "npm run build-css && ng build -c=production && npm run post-build",
    "post-build": "cp -a docs/browser/* docs/ && cp docs/index.html docs/404.html && rm docs/browser/* && rmdir docs/browser",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.15",
    "@angular/common": "^20.3.15",
    "@angular/compiler": "^20.3.15",
    "@angular/core": "^20.3.15",
    "@angular/forms": "^20.3.15",
    "@angular/platform-browser": "^20.3.15",
    "@angular/platform-browser-dynamic": "^20.3.15",
    "@angular/router": "^20.3.15",
    "@tailwindcss/cli": "^4.1.17",
    "@tailwindcss/postcss": "^4.1.17",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.3.13",
    "@angular/cli": "~20.3.13",
    "@angular/compiler-cli": "^20.3.15",
    "@types/jasmine": "~4.3.0",
    "autoprefixer": "^10.4.14",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.8.3"
  }
}
